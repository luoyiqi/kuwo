<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape>
            <solid android:color="@color/cropimg_savebtndef" />
            <stroke android:width="1.0dip" android:color="@color/cropimg_savebtndef" />
            <corners android:radius="3.0dip" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <solid android:color="@color/cropimg_cancelbtndef" />
            <stroke android:width="1.0dip" android:color="@color/cropimg_cancelbtndef" />
            <corners android:radius="3.0dip" />
        </shape>
    </item>
</selector>